{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Executar Launcher",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "cypress:open"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Renomear arquivos numericamente",
      "program": "${workspaceFolder}/renamer.js",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    }
  ]
}
